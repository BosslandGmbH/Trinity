<UserControl xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:uiComponents="clr-namespace:Trinity.UI.UIComponents"
             xmlns:controls="clr-namespace:Trinity.UI.UIComponents.Controls"
             mc:Ignorable="d" Foreground="{DynamicResource TextBush}" d:DesignHeight="300" d:DesignWidth="1400">
    <UserControl.Resources>
        <uiComponents:BindingProxy x:Key="Proxy" Data="{Binding}" />
    </UserControl.Resources>
    <DockPanel>
        <WrapPanel DockPanel.Dock="Top">
        </WrapPanel>
        <Grid DockPanel.Dock="Top">
            <Grid.ColumnDefinitions>
                <controls:ColumnDefinitionExtended Width="*" />            
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
           
            <!--<ut:RadarCanvas ItemsSource="{Binding LazyCache}" />-->
        </Grid>
    </DockPanel>
</UserControl>